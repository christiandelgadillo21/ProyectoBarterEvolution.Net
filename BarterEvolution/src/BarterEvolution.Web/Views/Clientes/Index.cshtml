@model IEnumerable<BarterEvolution.Infrastructure.Clientes>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombrecliente1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nombrecliente2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Apellidocliente1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Apellidocliente2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TelefonoMovil)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DireccionResidencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ciudad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GeneroNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdDocumentoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdLocalidadNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTipoclienteNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombrecliente1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombrecliente2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellidocliente1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellidocliente2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TelefonoMovil)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DireccionResidencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ciudad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GeneroNavigation.IdGenero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdDocumentoNavigation.IdTipoDocumento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdLocalidadNavigation.NombreLocalidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTipoclienteNavigation.IdTipocliente)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CedulaCliente">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CedulaCliente">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CedulaCliente">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
